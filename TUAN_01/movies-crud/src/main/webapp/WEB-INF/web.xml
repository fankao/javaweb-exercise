<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>movies-crud</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ActionManagementServlet</display-name>
    <servlet-name>ActionManagementServlet</servlet-name>
    <servlet-class>www.bt.servlets.ActionManagementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ActionManagementServlet</servlet-name>
    <url-pattern>/ActionManagementServlet/*</url-pattern>
  </servlet-mapping>
</web-app>